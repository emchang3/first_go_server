{{ define "navMenu" }}
  <div class="nav-menu-closed mdl-shadow--16dp" id="nav-menu">
    <h6 class="nav-section-header">Content</h6>
    <div id="nav-content">
      {{ range $page := .Pages }}
        <p class="nav-p">
          <a href="/post/{{ $page.File }}">
            {{ $page.Title }}
          </a>
        </p>
      {{ end }}
    </div>
    <a href="/about"><h6 class="nav-section-header">About</h6></a>
  </div>
{{ end }}
